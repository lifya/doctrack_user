<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.pln.www.TambahDokumentActivity">

    <TextView
        android:layout_width="170dp"
        android:layout_height="17dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="10dp"
        android:text="Konsultan"/>
    <EditText
        android:id="@+id/konsultan"
        android:layout_width="350dp"
        android:layout_height="35dp"
        android:layout_marginTop="35dp"
        android:layout_marginLeft="10dp" />
    <TextView
        android:layout_width="200dp"
        android:layout_height="17dp"
        android:layout_marginTop="70dp"
        android:layout_marginLeft="10dp"
        android:text="Nama Document/Pekerjaan"/>
    <EditText
        android:id="@+id/dokument"
        android:layout_width="350dp"
        android:layout_height="35dp"
        android:layout_marginTop="90dp"
        android:layout_marginLeft="10dp" />
    <TextView
        android:layout_width="170dp"
        android:layout_height="17dp"
        android:layout_marginTop="125dp"
        android:layout_marginLeft="10dp"
        android:text="No Kontrak"/>
    <TextView
        android:layout_width="20dp"
        android:layout_height="35dp"
        android:layout_marginTop="145dp"
        android:layout_marginLeft="10dp"
        android:text="No"
        android:id="@+id/textView4" />
    <EditText
        android:id="@+id/no1"
        android:layout_width="45dp"
        android:layout_height="35dp"
        android:layout_marginTop="145dp"
        android:layout_marginLeft="35dp" />
    <TextView
        android:layout_width="20dp"
        android:layout_height="35dp"
        android:layout_marginTop="145dp"
        android:layout_marginLeft="80dp"
        android:text="No"/>
    <EditText
        android:id="@+id/no2"
        android:layout_width="45dp"
        android:layout_height="35dp"
        android:layout_marginTop="145dp"
        android:layout_marginLeft="110dp" />
    <TextView
        android:id="@+id/cabang"
        android:layout_width="50dp"
        android:layout_height="35dp"
        android:text="Cabang"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="145dp"
        android:layout_marginLeft="115dp"/>
    <EditText
        android:id="@+id/no3"
        android:layout_width="80dp"
        android:layout_height="35dp"
        android:layout_marginTop="145dp"
        android:layout_marginLeft="230dp" />
    <TextView
        android:id="@+id/proses"
        android:layout_width="80dp"
        android:layout_height="30dp"
        android:text="Proses"
        android:layout_marginTop="190dp"
        android:layout_marginLeft="10dp"/>



</RelativeLayout>
